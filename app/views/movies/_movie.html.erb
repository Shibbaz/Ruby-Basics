<div id="<%= dom_id movie %>">
  <p>
    <strong>Imdb:</strong>
    <%= movie.imdb_id %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= movie.title %>
  </p>

  <p>
    <strong>Rating:</strong>
    <%= movie.rating %>
  </p>

  <p>
    <strong>Rank:</strong>
    <%= movie.rank %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= movie.year %>
  </p>

  <p>
    <strong>Data:</strong>
    <%= movie.data %>
  </p>

</div>
